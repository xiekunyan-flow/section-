{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++-11 生成活动文件",
			"command": "/usr/local/bin/g++-11",
			"args": [
				"-g",
				"${workspaceFolder}/*.cc",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${workspaceFolder}/test"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "编译器: /usr/local/bin/g++-11"
		}
	]
}